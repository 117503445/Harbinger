{
  "title": "Harbinger",
  "description": "Just Another FMLTutor For Future",
  "author": "https://github.com/TeamCovertDragon/Harbinger/graphs/contributors",
  "plugins": [
    "katex",
    "splitter",
    "expandable-chapters",
    "anchor-navigation-ex",
    "heimu",
    "yahei"
  ],
  "pluginsConfig": {
